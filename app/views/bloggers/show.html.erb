<div class="row">

  <div class="col s12 m8 l9">
    <div class="col s2">

    </div>
    <div class="col s11">
      <h1><%= current_user.first_name %>
      <% if current_user.bio.present? %>
        <%= current_user.bio %>
      <%end%>

      <% if @posts.any? %>
        <% @posts.each do |post| %>
      <ul>
        <li><%= link_to post.title, post_path(current_user, post) %></li>
        <%end%>
        <%end%>
        <h5><%= link_to 'Write Post', new_post_path(current_user) %></h5>
    </div>
  </div>

</div>
